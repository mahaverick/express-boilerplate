{
  "semi": true,
  "tabWidth": 2,
  "endOfLine": "lf",
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "importOrder": [
    "^(crypto/(.*)$)|^(crypto$)",
    "",
    "^(cookie-parser/(.*)$)|^(cookie-parser$)",
    "^(cors/(.*)$)|^(cors$)",
    "^(dotenv/(.*)$)|^(dotenv$)",
    "^(express/(.*)$)|^(express$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/types/(.*)$",
    "^@/configs/(.*)$",
    "^@/controllers/(.*)$",
    "^@/database/(.*)$",
    "^@/middlewares/(.*)$",
    "^@/repositories/(.*)$",
    "^@/services/(.*)$",
    "^@/routes/(.*)$",
    "^@/utils/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.0"
}
